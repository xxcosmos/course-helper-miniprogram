<view
        bind:touchmove="onTouchMove"
        class="van-rate custom-class"
>
    <van-icon
            bind:click="onSelect"
            class="van-rate__item"
            color="{{ disabled ? disabledColor : item ? color : voidColor }}"
            custom-class="icon-class"
            data-index="{{ index }}"
            name="{{ item ? icon : voidIcon }}"
            size="{{ size }}px"
            wx:for="{{ list }}"
            wx:key="index"
    />
</view>
