<view class="wux-class {{ classes.wrap }} {{ className }}">
    <view class="{{ classes.tail }}"></view>
    <view class="{{ classes.dot }}" style="{{ extStyle }}">
        <slot name="dot" wx:if="{{ custom }}"></slot>
    </view>
    <view class="{{ classes.content }}">
        <text>{{ content }}</text>
        <slot></slot>
    </view>
</view>
